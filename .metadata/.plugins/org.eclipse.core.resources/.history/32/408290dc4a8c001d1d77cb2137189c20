package com.array;

import java.util.Arrays;

public class ArraysAreEqual 
{
	public static void main(String[] args)
	{
		int a [] = {12,22,232,4,3};
		int arr [] = {12,22,232,4,3};
		
		boolean status = Arrays.equals(a, arr);
		
		if(status == true)
		{
			System.out.println("Are Equal");
		}
	}
}
