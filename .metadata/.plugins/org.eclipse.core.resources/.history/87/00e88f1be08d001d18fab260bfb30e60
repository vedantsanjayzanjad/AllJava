package com.stream;

import java.util.ArrayList;
import java.util.List;

class Employee
{
	private int empId;
	private String empName;
	private String empCity;
	
	public int getEmpId() {
		return empId;
	}
	public void setEmpId(int empId) {
		this.empId = empId;
	}
	public String getEmpName() {
		return empName;
	}
	public void setEmpName(String empName) {
		this.empName = empName;
	}
	public String getEmpCity() {
		return empCity;
	}
	public void setEmpCity(String empCity) {
		this.empCity = empCity;
	}
	
	public Employee(int empId, String empName, String empCity) {
		super();
		this.empId = empId;
		this.empName = empName;
		this.empCity = empCity;
	}
	
	
	
}



public class GroupBy 
{
	public static void main(String[] args) 
	{
		List<Employee> list = new ArrayList<Employee>();
		list.add(new Employee(1,"Vedant","Pune"));
		list.add(new Employee(1,"AKshay","Pune"));
		list.add(new Employee(1,"Rushikesh","Amravati"));
		list.add(new Employee(1,"Nobody","Nanded"));
	}
}
