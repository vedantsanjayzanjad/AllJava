package com.array;

public class MaxAndMin 
{
	public static void main(String[] args) 
	{
		int arr [] = {12,234,56,35,63};
		int max = arr[0];
		
		for(int i : arr)
		{
			System.out.println(i);
			if(i > max)
			{
				max = i;
			}
		}
		System.out.println("Maximum Element in an araay " + max);
	}
}
