package com.singleton;

public class Singleton
{
	private static Singleton singleton;
	
	private Singleton()
	{
		
	}
	
	public static synchronized Singleton getData()
	{
		if(singleton == null)
		{
			singleton = new Singleton();
		}
		return singleton;
	}
	
	public static void main(String[] args) 
	{
		
	}
}
