package com.array;

public class EvenAndOddPosition 
{
	public static void main(String[] args) 
	{
		int arr [] = {12,234,56,35,63};
		
		for(int i = 0;i <arr.length;i++)
		{
			if(i % 2 == 0)
			{
				System.out.println("Elements in even position " + arr[i]);
			}
			else 
			{
				System.out.println(("Elements in even position " + arr[i]);
			}
		}
	}
}
