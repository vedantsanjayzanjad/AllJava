package com.basic;

public class PolindromString {
	public static void main(String[] args) {

		String str = "Vedant";
		String rev = "";
		String temp = str; 
		
		for(int i = str.length() - 1 ; i >= 0;i--)
		{
			rev = rev + str.charAt(i);
		}
		System.out.println(rev);
		
		if(temp.equals(str))
	}
}
