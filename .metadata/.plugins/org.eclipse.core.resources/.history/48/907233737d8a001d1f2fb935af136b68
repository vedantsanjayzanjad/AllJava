package com.stream;

import java.util.*;

public class CustomizeSorting
{
	public static void main(String[] args) 
	{
		
		List<String>list = new ArrayList<String>();
		list.add("Vedant");
		list.add("Rushi");
		list.add("Aavani");
		list.add("Jay");
		System.out.println(list);
		
		Comparator<String> custList = (s1,s2) ->
		{
			int a1 = s1.length();
			int a2 = s2.length();
		}
	}
}
